{
    "name": "miguelsira86/laravel-gettext",
    "description": "Adds localization support to laravel applications in an easy way using Poedit and GNU gettext.",
    "version": "v1.0.0",
    "keywords": [
        "gettext",
        "localization",
        "poedit",
        "laravel-gettext",
        "laravel", "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Sira",
            "email": "miguelsira86@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "laravel/helpers": "^1.1",
        "gettext/languages": "^2.10"
    },
    "require-dev": {
        "mockery/mockery": "1.6.12",
        "phpunit/phpunit": "~7.0 || ^8.5 || ^9",
        "squizlabs/php_codesniffer" : "1.5.*",
        "laravel/laravel": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-0": {
            "Xinax\\LaravelGettext\\": "src/"
        },
        "files": [
            "src/Xinax/LaravelGettext/Support/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
          "providers": [
            "Xinax\\LaravelGettext\\LaravelGettextServiceProvider"
          ]
       }
    }
}
